{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyNearlyAll",
            "Effect": "Deny",
            "Action": "*",
            "Resource": "*",
            "Condition": {
                "ArnNotLike": {
                    "aws:PrincipalARN": "arn:aws:iam::*:root"
                }
            }
        }
    ]
}